  static MatchFlags(left : bool, right : bool) : bool
  {
	def test(e1, e2)
	{
	  match (e1, e2)
	  {
	    | (true, true)   => true
		| (false, false) => false
		| (true, false) with arg = "second"
		| (false, true) with arg = "first" =>
		  throw System.ArgumentException("The first & second mast be same length.", arg)
	  }
	}
	
	test(left, right)
  }
